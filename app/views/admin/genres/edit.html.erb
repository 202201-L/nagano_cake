<div class="container mb-5">
  <h3 class="mt-4 mb-5">ジャンル編集</h3>
  <table class="table">
    <row>
      <%= form_with model: @genre, url: admin_genre_path, method: :patch do |f| %>
        <small class="mr-5"><%= f.label :ジャンル名 %></small>
        <%= f.text_field :name, class:"mr-5" %>
        <%= f.submit "変更を保存", class:"btn btn-success" %>
      <% end %>
    </row>
  </table>
</div>