<div class="container">
  <div class="row">
    <div class="col-md-10 m-3">
      <h3><%= @end_user.last_name %><%= @end_user.first_name %>さんの会員情報</h3>
    </div>
  </div>
      <%= form_with model: @end_user,url: admin_end_user_path(@end_user.id), local:true do |f| %>
        <div class="form-group row">
        <%= f.label :name, '氏名', class: "col-md-3" %>
        <div class="col-md-4">
          <%= f.label :last_name, '(性)' %>
          <%= f.text_field :last_name, class: "form-control" %>
        </div>
        <div class="col-md-4">
          <%= f.label :first_name, '(名)' %>
          <%= f.text_field :first_name, class: "form-control" %>
        </div>
      </div>
      <div class="form-group row">
        <%= f.label :name, 'フリガナ', class: "col-md-3" %>
        <div class="col-md-4">
          <%= f.label :first_name_kana, '(セイ)' %>
          <%= f.text_field :last_name_kana, class: "form-control" %>
        </div>
        <div class="col-md-4">
          <%= f.label :first_name_kana, '(メイ)' %>
          <%= f.text_field :first_name_kana, class: "form-control" %>
        </div>
      </div>
      <div class="form-group row">
      <%= f.label :post_code, '郵便番号', class: "col-md-3" %>
       <%= f.text_field :post_code, class: "form-control col-md-3" %>
      </div>
      <div class="form-group row">
      <%= f.label :address, '住所', class: "col-md-3" %>
      <%= f.text_field :address, class: "form-control col-md-8" %>
      </div>
      <div class="form-group row">
      <%= f.label :tel, '電話番号', class: "col-md-3" %>
      <%= f.text_field :tel, class: "form-control col-md-3" %>
      </div>
      <div class="form-group row">
      <%= f.label :email, 'メールアドレス', class: "col-md-3" %>
      <%= f.text_field :email, class: "form-control col-md-3" %>
      </div>
      <div class="form-group">
      <%= f.label :会員ステータス %>
      <%= f.radio_button :is_delete, :false %>
      <%= f.label :is_delete, "有効" %>
      <%= f.radio_button :is_delete, :true %>
      <%= f.label :is_delete, "退会" %>
      </div>

      <div class="form-group">
      <%= f.submit '編集内容を保存する',class: 'btn btn-success' %>
      </div>
      <% end %>