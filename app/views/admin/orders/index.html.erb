<h1>test</h1>
<h1>注文履歴一覧</h1>

<table border="1" width="400">
  <thead>
    <tr>
      <th>購入日時</th>
      <th>購入者</th>
      <th>注文個数</th>
      <th>注文ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td>
          <%= link_to admin_order_path(order.id) do %>
          <%= order.created_at %>
          <% end%>
          </td>
        <td><%= order.end_user_id %></td>
        <td><%= order.order_details.amount %></td>
        <td><%= order.order_status %></td>
      </tr>
    <% end%>
  </tbody>
</table>

<p><%= paginate @orders %></p>