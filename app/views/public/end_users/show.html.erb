<div class="container">
  <div class="row">
    <h3>マイページ</h3>
  </div>
<div class="row">
  <h4>登録情報</h4>
  <%= link_to '編集する', edit_end_user_path(@end_user), class: "btn btn-success ml-5"%>
</div>

<p>
<table border="1" width="400">
    <tr>
      <td>氏名</td>
      <td><%= @end_user.last_name %><%= @end_user.first_name %></td>
    </tr>
    <tr>
      <td>カナ</td>
      <td><%= @end_user.last_name_kana %><%= @end_user.first_name_kana %></td>
    </tr>
    <tr>
      <td>郵便番号</td>
      <td><%= @end_user.post_code %></td>
    </tr>
    <tr>
      <td>住所</td>
      <td><%= @end_user.address %></td>
    </tr>
    <tr>
      <td>電話番号</td>
      <td><%= @end_user.tel %></td>
    </tr>
    <tr>
      <td>メールアドレス</td>
      <td><%= @end_user.email %></td>
    </tr>
  </table>
  </p>
  <div class="row m-4">
   <p>配送先</p>
     <%= link_to '一覧を見る', deliveries_path, class: "col-md-2 offset-1 btn btn-primary" %>
  </div>
  <div class="row m-2">
   <p>注文履歴</p>
     <%= link_to '一覧を見る', orders_path(@end_user), class: "col-md-2 offset-1 btn btn-primary" %>

  </div>

</div>