<h1>マイページ</h1>
<table border="1" width="400">
    <tr>
      <td>氏名</td>
      <td><%= @end_user.last_name %><%= @end_user.first_name %></td>
    </tr>
    <tr>
      <td>フリガナ</td>
      <td><%= @end_user.last_name_kana %><%= @end_user.first_name_kana %></td>
    </tr>
    <tr>
      <td>郵便番号</td>
      <td><%= @end_user.post_code %></td>
    </tr>
    <tr>
      <td>住所</td>
      <td><%= @end_user.address %></td>
    </tr>
    <tr>
      <td>電話番号</td>
      <td><%= @end_user.tel %></td>
    </tr>
    <tr>
      <td>メールアドレス</td>
      <td><%= @end_user.email %></td>
    </tr>
    <tr>
      <td>会員ステータス</td>
      <td><%= @end_user.is_delete %></td>
    </tr>