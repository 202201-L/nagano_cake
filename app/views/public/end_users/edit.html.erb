<h1>テスト</h1>

<h1><%= @end_user.last_name %><%= @end_user.first_name %>さんの会員情報</h1>

<%= form_with model: @end_user,url: end_user_path(@end_user.id), local:true do |f| %>

      <div class="form-group">
        <%= f.label :姓 %>
        <%= f.text_field :last_name, class: "form-control last_name" %>
      </div>
       <div class="form-group">
        <%= f.label :名 %>
        <%= f.text_field :first_name, class: "form-control first_name" %>
      </div>
      
       <div class="form-group">
        <%= f.label :姓カナ%>
        <%= f.text_field :last_name_kana, class: "form-control last_name_kana" %>
      </div>
       <div class="form-group">
        <%= f.label :名カナ %>
        <%= f.text_field :first_name_kana, class: "form-control first_name_kana" %>
      </div>
      
     <div class="form-group">
        <%= f.label :郵便番号%>
        <%= f.text_field :post_code, class: "form-control-file post_code" %>
      </div>
      <div class="form-group">
        <%= f.label :住所%>
        <%= f.text_area :address, class: "form-control introduction" %>
      </div>
      <div class="form-group">
        <%= f.label :メールアドレス%>
        <%= f.text_area :email, class: "form-control email" %>
      </div>
      <div class="form-group">
        <%= f.submit class: 'btn btn-secondary' %>
      </div>
    <% end %>