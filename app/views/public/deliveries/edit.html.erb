<h2>配送先編集</h2>
<%= form_with model: @delivery, url: delivery_path(@delivery), method: :patch do |f| %>
  <label>郵便番号(ハイフンなし)</label>
  <%= f.text_field :post_code %><br>
  <label>住所</label>
  <%= f.text_field :address %><br>
  <label>宛名</label>
  <%= f.text_field :address_name %><br>
  <%= f.submit "変更内容を保存" %>
<% end %>
